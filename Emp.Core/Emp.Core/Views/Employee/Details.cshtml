@model EmployeeDetailsViewModel

@{ 
    ViewBag.PageTile = Model.PageTitle;
    var employee = Model.Employee;
}

<table border="1">
    <tr>
        <td>Id</td>
        <td>@employee.EmpId</td>
    </tr>
    <tr>
        <td>Name</td>
        <td>@employee.Name</td>
    </tr>
    <tr>
        <td>Email</td>
        <td>@employee.Email</td>
    </tr>
    <tr>
        <td>Department</td>
        <td>@employee.Department</td>
    </tr>

</table>

